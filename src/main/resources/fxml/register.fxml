<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="592.0" prefWidth="606.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.register.RegisterController">
    <children>
        <Label alignment="CENTER" layoutX="185.0" layoutY="44.0" prefHeight="73.0" prefWidth="237.0" text="Register">
            <font>
                <Font size="43.0" />
            </font>
        </Label>
        <VBox layoutX="154.0" layoutY="164.0" prefHeight="336.0" prefWidth="370.0" spacing="30.0">
            <children>
                <HBox spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Username" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="usText" />
                    </children>
                </HBox>
                <HBox layoutX="10.0" layoutY="10.0" spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Password" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="pwText" />
                    </children>
                </HBox>
                <HBox layoutX="10.0" layoutY="41.0" spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="First N" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="firstText" />
                    </children>
                </HBox>
                <HBox layoutX="10.0" layoutY="72.0" spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Last N" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="lastText" />
                    </children>
                </HBox>
                <HBox layoutX="10.0" layoutY="103.0" spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Phone" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="phoneText" />
                    </children>
                </HBox>
                <HBox layoutX="10.0" layoutY="134.0" spacing="20.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Adress" textFill="#09a706">
                            <font>
                                <Font size="21.0" />
                            </font>
                        </Label>
                        <TextField fx:id="addressText" />
                    </children>
                </HBox>
            </children>
        </VBox>
        <Button fx:id="confirmButton" layoutX="245.0" layoutY="516.0" mnemonicParsing="false" onAction="#confirmButtonClicked" prefHeight="46.0" prefWidth="149.0" style="-fx-background-color: cyan;" text="Confirm" />
        <Button fx:id="backButton" layoutX="38.0" layoutY="19.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="31.0" prefWidth="525.0" text="Back" />
    </children>
</AnchorPane>
